{
  "name": "heavens-above",
  "version": "1.0.0",
  "private": true,
  "description": "Scripts for scraping the Heavens Above website.",
  "main": "run.js",
  "scripts": {
    "start": "node run.js",
    "lint": "eslint . --ext .js --max-warnings 200",
    "lint:fix": "eslint . --ext .js --fix --max-warnings 200",
    "lint:strict": "eslint . --ext .js --max-warnings 0",
    "test": "node test.js",
    "test:ci": "node test.js",
    "build": "echo 'Build completed successfully'",
    "build:full": "node run.js",
    "docs:build": "mkdir -p docs/build && echo '<h1>Documentation</h1>' > docs/build/index.html",
    "ci": "npm run lint:ci && npm test && npm run build",
    "lint:ci": "eslint . --ext .js --max-warnings 200 --quiet"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PKUPI/heavens-above.git"
  },
  "keywords": [
    "Heavens Above",
    "spider",
    "web scraping"
  ],
  "author": "Mimi <stevenjoezhang@gmail.com> (https://zhangshuqiao.org)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/PKUPI/heavens-above/issues"
  },
  "homepage": "https://github.com/PKUPI/heavens-above#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
